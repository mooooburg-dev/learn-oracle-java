
class TheShape {
	String name;
	
	public void draw() {
		System.out.println("TheShape.draw()");
	}
}
class TheRect extends TheShape {
	public void draw() {
		System.out.println("TheShape.draw()");
	}
}
class TheOval extends TheShape {
	public void draw() {
		System.out.println("TheShape.draw()");
	}
}
class TheLine extends TheShape {
	public void draw() {
		System.out.println("TheShape.draw()");
	}
}

public class Polymorphism {

	public static void main(String[] args) {

	}

}
