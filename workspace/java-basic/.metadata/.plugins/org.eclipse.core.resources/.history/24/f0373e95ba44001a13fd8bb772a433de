
public class MethodDemo {

	public static void main(String[] args) {
		
		for (int y=0 ; y<20 ; y++) {
			for(int x=0 ; x<10 ; x++) {
				if( x == 0 || x == 19 || y == 0 || y ==9 ) {
					System.out.print("*");
				}
				else {
					System.out.print(" ");
				}
			}
			System.out.println();
		}

	}

}
